#~ msgstr "Clarustus"

#~ msgid "Merlunius"
#~ msgstr "Merlunius"

#~ msgid "Aimucasur"
#~ msgstr "Aimucasur"

#~ msgid "Secadius"
#~ msgstr "Secadius"

